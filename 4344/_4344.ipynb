      "source": [
        "# 4344 평균은 넘겠지\n",
        "C = int(input())\n",
        "\n",
        "for i in range(C):\n",
        "  N = list(map(int, input().split(' ')))\n",
        "  avg = sum(N[1:]) / N[0]\n",
        "  count = 0\n",
        "  for j in N[1:]:\n",
        "    if j > avg:\n",
        "      count += 1\n",
        "  \n",
        "  print(str('%.3f'%round((count/N[0])*100, 3)) + '%')"
      ],
